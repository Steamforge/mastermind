{
  "name": "mastermind",
  "version": "1.0.0",
  "description": "A version of the old Mastermind game",
  "main": "index.js",
  "scripts": {
    "build": "gatsby build",
    "build-debug": "gatsby build --no-uglify --verbose --log-pages",
    "dev": "gatsby develop -H 0.0.0.0 -p 8000 -o",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1",
    "lint": "eslint \"./src/**\" -o ./test/eslintreport.html -f html",
    "lint-fix": "eslint \"src/**\" -o ./test/eslintreport.html -f html --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Steamforge/mastermind.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Steamforge/mastermind/issues"
  },
  "homepage": "https://mastermind.studley.dev",
  "dependencies": {
    "classnames": "^2.2.6",
    "cross-env": "^7.0.3",
    "gatsby": "^3.1.1",
    "gatsby-plugin-google-analytics": "^3.1.0",
    "gatsby-plugin-manifest": "^3.1.0",
    "gatsby-plugin-no-sourcemaps": "^3.1.0",
    "gatsby-plugin-react-helmet": "^4.1.0",
    "gatsby-plugin-robots-txt": "^1.5.5",
    "gatsby-plugin-sass": "^4.1.0",
    "gatsby-plugin-webfonts": "^2.0.0",
    "gatsby-source-filesystem": "^3.1.0",
    "prettier": "^2.2.1",
    "react": "^17.0.1",
    "react-beautiful-dnd": "^13.1.0",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.1.0",
    "sass": "^1.32.8"
  },
  "devDependencies": {
    "eslint": "^7.22.0",
    "eslint-config-react-app": "^6.0.0"
  }
}
